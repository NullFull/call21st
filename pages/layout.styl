@import "consts.styl"


reset()
  html, body, div, ul, li, input, h1, h2, h3, h4, h5
    margin 0
    padding 0
    box-sizing border-box

:global
  reset()

  body
    font-family "Noto Sans KR", sans-serif

  li
    list-style none

  a
    color inherit

  strong
    background #E5CCA6
    color color-strong

.container
  margin 0 auto
  padding 20px 20px 40px 20px
  max-width 640px
  > h1
    margin 20px 0 20px 0
    text-align center
  > h3
    margin 20px 0 20px 0
    text-align center
